<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %logger{36}: %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="org.hisp.gfexchange" level="info" additivity="true"/>
    <Logger name="org.hisp.dhis" level="debug" additivity="true"/>
    <Root level="warn">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
