{
  "metadata": {
    "name": "{country} to GF global",
    "version": "1.0"
  },
  "source": {
    "api": {
      "url": "https://{country_server_base_url}",
      "username": "{username}",
      "password": "{password}"
    },
    "request": {
      "dx": [
        "GF_CM-1a",
        "GF_CM-1b"
      ],
      "pe": [
        "{period_id_like_202009}"
      ],
      "ou": [
        "{country_org_unit_uid}"
      ],
      "filters": [
        {
          "dimension": "{dimension_id_1}",
          "items": [
            "{dimension_item_id_1}",
            "{dimension_item_id_2}"
          ]
        }, {
          "dimension": "{dimension_id_2}",
          "items": [
            "{dimension_item_id_1}",
            "{dimension_item_id_2}"
          ]
        }          
      ],
      "outputIdScheme": "CODE",
      "inputIdScheme": "CODE"
    }
  },
  "target": {
    "api": {
      "url": "https://devgf.baosystems.com",
      "username": "{username}",
      "password": "{password}"
    },
    "request": {
      "orgUnitIdScheme": "UID",
      "idScheme": "CODE"
    }
  }  
}
