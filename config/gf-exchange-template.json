{
  "metadata": {
    "name": "{country} to GF global",
    "version": "1.0"
  },
  "source": {
    "api": {
      "url": "https://{country_server_base_url}",
      "username": "{username}",
      "password": "{password}"
    },
    "request": {
      "dx": [
        "GF_CM-1a",
        "GF_CM-1b",
        "GF_CM-1c",
        "GF_KP-1a",
        "GF_KP-1b",
        "GF_KP-1c",
        "GF_KP-1d",
        "GF_KP-3a",
        "GF_KP-3b",
        "GF_KP-3c",
        "GF_KP-3d",
        "GF_MDR_TB-3",
        "GF_PMTCT-2.1",
        "GF_RE-ART",
        "GF_RE-KP",
        "GF_RE-MAL",
        "GF_RE-TB",
        "GF_RR-ART",
        "GF_RR-KP",
        "GF_RR-MAL",
        "GF_RR-TB",
        "GF_TB/HIV-3.1",
        "GF_TCP-1",
        "GF_TCP-7a",
        "GF_TCP-7b",
        "GF_TCP-7c",
        "GF_TCS-1.1",
        "GF_TCS-New",
        "GF_VC-3",
        "GF_VC-5",
        "GF_YP-2",
        "GF_YP-3"
      ],
      "pe": [
        "{period_id_like_202009}"
      ],
      "ou": [
        "{country_org_unit_uid}"
      ],
      "outputIdScheme": "CODE",
      "inputIdScheme": "CODE"
    }
  },
  "target": {
    "api": {
      "url": "https://devgf.baosystems.com",
      "username": "{username}",
      "password": "{password}"
    },
    "request": {
      "orgUnitIdScheme": "UID",
      "idScheme": "CODE"
    }
  }  
}
